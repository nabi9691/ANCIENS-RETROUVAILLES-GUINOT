{% extends 'base.html.twig' %}

{% block title %}

afficher médias :
{% endblock %}

{% block content %}
    <h1>
    Afficher médias :
    </h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th> Titre </th>
                <th> Contenu </th>
                <th> Date </th>
                 <th> Status </th>
                <th> Catégories </th>
                 <th> Auteurs </th>
                 <th> Utilisateurs </th>
                 </tr>
        </thead>
        <tbody>
            <tr>
<td>{{ medias.id }}</td>
<td>{{ medias.imageName }}</td>
                        <td>{{ medias.contenu }}</td>
<td>{{ medias.date|date("d/m/Y") }}</td>
<td>{{ medias.status }}</td>
<td>{{ medias.categories }}</td>
<td>{{ medias.auteurs }}</td>
                        
                        </tr>
        </tbody>
    </table>

    {% endblock %}
